{
  "project": {
    "name": "BAS Automation Project",
    "version": "1.0.0",
    "description": "Browser Automation Studio project with optimized configuration"
  },
  "browser": {
    "engine": "Chrome",
    "platform": "Microsoft Windows",
    "minimumVersion": "current",
    "maximumVersion": "current",
    "tags": ["Chrome", "Microsoft Windows"]
  },
  "fingerprint": {
    "enabled": true,
    "perfectCanvas": true,
    "canvasEmulation": "accurate"
  },
  "configuration": {
    "suppressWarnings": true,
    "debugMode": false,
    "logLevel": "info"
  },
  "proxy": {
    "enabled": false,
    "type": "http",
    "url": ""
  },
  "timeout": {
    "pageLoad": 30000,
    "elementWait": 10000,
    "actionDelay": 1000
  }
}